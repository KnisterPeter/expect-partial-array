{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    // "lib": [],
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,

    "strict": true,

    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,

    "moduleResolution": "node",
    "esModuleInterop": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["index.js", "test.js"]
}
